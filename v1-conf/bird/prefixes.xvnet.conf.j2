function is_xvnet_prefix() {
    case net.type {
        NET_IP4: return net ~ [ 172.20.206.64/26 ];
        NET_IP6: return net ~ [ fd00:443a:ef14::/48 ];
        else: print "is_xvnet_prefix: unexpected net.type ", net.type, " ", net; return false;
    }
}

function is_xvnet_internal_prefix() {
    case net.type {
        NET_IP4: return net ~ [ 172.20.206.64/26{27,32} ];
        NET_IP6: return net ~ [ fd00:443a:ef14::/48{49,128} ];
        else: print "is_xvnet_internal_prefix: unexpected net.type ", net.type, " ", net; return false;
    }
}
