{
	"root": true,
	"extends": [
		"wikimedia/client-es6",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2022
	},
	"env": {
		"browser": true,
		"commonjs": true
	},
	"globals": {
		"Tree": "readonly"
	},
	"rules": {
		"es-x/no-array-prototype-includes": 0,
		"es-x/no-class-fields": 0,
		"es-x/no-rest-spread-properties": 0
	}
}
