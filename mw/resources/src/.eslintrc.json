{
	"root": true,
	"extends": [
		"wikimedia/client-es6",
		"wikimedia/jquery",
		"wikimedia/mediawiki",
		"wikimedia/jsdoc"
	],
	"globals": {
		"require": "readonly",
		"module": "readonly"
	},
	"rules": {
		"es-x/no-promise-prototype-finally": "off",
		"max-len": "off",
		"no-var": "off",
		"no-jquery/no-global-selector": "off"
	}
}
